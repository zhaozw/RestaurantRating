<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MessageBoxText">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">20sp</item>
    </style>
    
	<style name="AppTitleLayout">
      <item name="android:layout_height">50dip</item>
      <item name="android:gravity">center</item>
      <item name="android:background">#9789a2</item>
   	</style>
    
	<style name="AppTitle">
      <item name="android:textColor">#000000</item>
      <item name="android:textSize">35sp</item>
      <item name="android:paddingTop">0dip</item>
      <item name="android:paddingBottom">0dip</item>
      <item name="android:gravity">center</item>
   	</style>

	<style name="SearchLayout">
      <item name="android:gravity">center</item>
      <item name="android:background">#9789a2</item>
   	</style>
	
	<style name="Search">
      <item name="android:textColor">#000000</item>
      <item name="android:textSize">20sp</item>
      <item name="android:paddingTop">0dip</item>
      <item name="android:paddingBottom">0dip</item>
      <item name="android:gravity">center_vertical|left</item>
   	</style>

	
	<style name="TextRate">
      <item name="android:textSize">75sp</item>
      <item name="android:paddingBottom">0dip</item>
      <item name="android:paddingLeft">10dip</item>
      <item name="android:paddingRight">20dip</item>
	  <item name="android:gravity">top|center_horizontal</item>
      <item name="android:textColor">#ffffff</item>
	</style>
	
	<style name="TextRate.Big">
      <item name="android:textSize">95sp</item>
   	</style>
   			
	<style name="TextUser">
      <item name="android:textSize">36sp</item>
      <item name="android:paddingTop">0dip</item>
      <item name="android:paddingBottom">0dip</item>
      <item name="android:gravity">left</item>
      <item name="android:textColor">#ffffff</item>
    </style>

	<style name="TextUserUnder">
      <item name="android:textSize">12sp</item>
      <item name="android:paddingTop">0dip</item>
      <item name="android:paddingBottom">0dip</item>
      <item name="android:gravity">center</item>
      <item name="android:textColor">#ffffff</item>
  	</style>
		
	<style name="TextUserUnder.Blue">
      <item name="android:textColor">#00c6d2</item>
   	</style>

	<style name="TextUserUnder.Blue.Left">
      <item name="android:gravity">center_vertical|left</item>
   	</style>
		
	<style name="UserRateList">
      <item name="android:background">#ffffff</item>
   	</style>
		
	<style name="TextUser.Small">
      <item name="android:textSize">13sp</item>
   	  <item name="android:gravity">center</item>
    </style>
			
	<style name="TextRestaurant">
      <item name="android:textSize">40sp</item>
      <item name="android:paddingTop">0dip</item>
      <item name="android:paddingBottom">0dip</item>
      <item name="android:gravity">left</item>
      <item name="android:textColor">#ffffff</item>
    </style>	

	<style name="TextRateItem">
      <item name="android:textSize">40sp</item>
      <item name="android:paddingTop">0dip</item>
      <item name="android:paddingBottom">0dip</item>
      <item name="android:paddingLeft">10dip</item>
      <item name="android:gravity">left</item>
      <item name="android:textColor">#000000</item>
    </style>

	<style name="TextUserUnder.Big">
      <item name="android:textSize">24sp</item>
  	</style>
	
		
	
</resources>